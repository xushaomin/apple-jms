<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:c="http://www.springframework.org/schema/c" 
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:cache="http://www.springframework.org/schema/cache"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd
        http://www.springframework.org/schema/cache http://www.springframework.org/schema/cache/spring-cache.xsd"
     default-lazy-init="true">
     
	<bean id="dataCarrierFactoryBean" class="com.appleframework.jms.datacarrier.factory.ObjectDataCarrierFactoryBean">
		<property name="consumer" ref="messageConsumer" />
	</bean>
	
	<bean id="messageProducer" class="com.appleframework.jms.datacarrier.producer.ObjectMessageProducer">
		<property name="carrier" ref="dataCarrierFactoryBean" />
	</bean>	
	
	<bean id="messageConsumer" class="com.appleframework.jms.datacarrier.MessageConsumerTest">
	</bean>
		
</beans>